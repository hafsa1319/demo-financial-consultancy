range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div class=\"\" mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"mbr-row mbr-jc-c\" mbr-class=\"{'mbr-jc-c' : cardsAmount<3}\">\n            <div class=\"mbr-col-sm-12 card\" mbr-class=\"{'mbr-col-lg-4' : cardsAmount<4, 'mbr-col-lg-3' : cardsAmount==4, 'mbr-col-md-6': cardsAmount ==4 || cardsAmount < 4}\">\n                <div class=\"inner-item rounded__10\">\n                    <div class=\"card-img\">\n                        <div class=\"image item1\">\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/people1.jpg\" alt=\"image\" height=\"279.609\" width=\"328.95176470588234\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n                                <a href=\"#\">\n                                    <img src=\"@PROJECT_PATH@/assets/images/people1.jpg\" class=\"mbr-temp\">\n                                </a>\n                            </amp-img>\n                            <div class=\"date\" mbr-if=\"showDate\">\n                                <h5 class=\"mbr-fonts-style mbr-white number-date\" mbr-theme-style=\"display-5\" data-app-selector=\"\">\n                                    24\n                                </h5>\n                                <h6 class=\"mbr-fonts-style mbr-white mbr-bold\" mbr-theme-style=\"display-4\">\n                                    FEB\n                                </h6>\n                            </div>\n                        </div>\n                        <div class=\"card-text\">\n                            <div class=\"card-text-box\">\n                                <h6 mbr-if=\"showCardsTitle\" class=\"mbr-fonts-style card-title mbr-bold mbr-black\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title\">\n                                    <a href=\"\" class=\"text-danger\">BUSINESS_NEWS</a>\n                                </h6>\n                                <h5 class=\"mbr-fonts-style mbr-black title-text\" mbr-theme-style=\"display-5\" data-app-selector=\"\" mbr-if=\"showCardsText\">\n                                    <a href=\"\" class=\"text-info link-text\">3 Clear Warnings That Say Your Goals Are Too Small</a> \n                                </h5>\n                                <div mbr-buttons=\"\" mbr-theme-style=\"display-4\" class=\"mbr-section-btn animation-link\" mbr-if=\"showCardsLink\" data-toolbar=\"-mbrBtnMove,-mbrBtnRemove,-mbrBtnAdd\"><a class=\"btn-underline text-primary mbr-bold \" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\"><span class=\"read-more-arrow\"></span>READ MORE</a></div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"mbr-col-sm-12 card\" mbr-if=\"cardsAmount>1\" mbr-class=\"{'mbr-col-lg-4' : cardsAmount<4, 'mbr-col-lg-3' : cardsAmount==4, 'mbr-col-md-6': cardsAmount ==4 || cardsAmount < 4}\">\n                <div class=\"inner-item rounded__10\">\n                    <div class=\"card-img\">\n                        <div class=\"image item2\">\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/people3.jpg\" alt=\"image\" height=\"279.609\" width=\"328.95176470588234\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n                                <a href=\"#\"><img src=\"@PROJECT_PATH@/assets/images/people3.jpg\" class=\"mbr-temp\"></a>\n                            </amp-img>\n                            <div class=\"date\" mbr-if=\"showDate\">\n                                <h5 class=\"mbr-fonts-style mbr-white number-date\" mbr-theme-style=\"display-5\" data-app-selector=\"\">\n                                    24\n                                </h5>\n                                <h6 class=\"mbr-fonts-style mbr-white mbr-bold\" mbr-theme-style=\"display-4\">\n                                    FEB\n                                </h6>\n                            </div>\n                        </div>\n                        <div class=\"card-text\">\n                            <div class=\"card-text-box\">\n                                <h6 mbr-if=\"showCardsTitle\" class=\"mbr-fonts-style card-title mbr-bold mbr-black\" mbr-theme-style=\"display-4\" data-app-selector=\".card-title\">\n                                    <a href=\"\" class=\"text-danger\">ANALYSIS_NEWS</a>\n                                </h6>\n                                <h5 class=\"mbr-fonts-style mbr-black title-text\" mbr-theme-style=\"display-5\" data-app-selector=\"\" mbr-if=\"showCardsText\">\n                                    <a href=\"\" class=\"text-info link-text\">10 Things Successful Mompreneurs Do Different</a> \n                                </h5>\n                                <div mbr-buttons=\"\" mbr-theme-style=\"display-4\" class=\"mbr-section-btn animation-link\" mbr-if=\"showCardsLink\" data-toolbar=\"-mbrBtnMove,-mbrBtnRemove,-mbrBtnAdd\"><a class=\"btn-underline text-primary mbr-bold \" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\"><span class=\"read-more-arrow\"></span>READ MORE</a></div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"mbr-col-sm-12 card\" mbr-if=\"cardsAmount>2\" mbr-class=\"{'mbr-col-lg-4' : cardsAmount<4, 'mbr-col-lg-3' : cardsAmount==4, 'mbr-col-md-6': cardsAmount ==4 || cardsAmount < 4}\">\n                <div class=\"inner-item rounded__10\">\n                    <div class=\"card-img\">\n                        <div class=\"image item3\">\n  