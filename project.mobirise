          "_customHTML": "<section class=\"testimonials1 slider slider-with-bullets\" group=\"Testimonials\" plugins=\"amp-main,amp-carousel,amp-bind\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n    <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"15\" value=\"13\" step=\"1\">\n    <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"15\" value=\"13\" step=\"1\">\n    <header>Show/Hide</header>\n    <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n    <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubTitle\" checked=\"\">\n    <input type=\"checkbox\" title=\"Face\" name=\"showFace\" checked=\"\">\n    <input type=\"checkbox\" title=\"Subtitle Card\" name=\"showSubTitleCard\" checked=\"\">\n    <header>Carousel</header>\n    <input type=\"range\" min=\"2\" max=\"6\" step=\"1\" value=\"3\" title=\"Slides Amount\" name=\"imagesAmount\" required=\"\">\n    <input type=\"checkbox\" title=\"Show Author\" name=\"showAuthor\" checked=\"\">\n    <select title=\"Content Align\" name=\"alignContent\">\n    <option value=\"1\">Left</option>\n    <option value=\"2\" selected=\"\">Center</option>\n    <option value=\"3\">Right</option>\n    </select>\n    <input type=\"range\" inline=\"\" title=\"Carousel Height\" name=\"slideHeight\" min=\"200\" max=\"600\" value=\"450\" step=\"20\">\n    <header>Color</header>\n    <input type=\"color\" title=\"Controls\" name=\"controlsColor\" value=\"#222222\">\n    <input type=\"color\" title=\"Quote\" name=\"colorQuote\" value=\"#d2bea6\">\n\n    <header>Background</header>   \n    <fieldset type=\"background\" name=\"bg\">\n    <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/map.jpg\" selected=\"\">\n    <input type=\"color\" title=\"Color\" value=\"#f5f5f5\">\n    </fieldset>\n    <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n    <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    \n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div class=\"face__background\" mbr-if=\"showFace\">\n        <amp-img src=\"@PROJECT_PATH@/assets/images/face-1.png\" alt=\"image\" height=\"110\" width=\"109.12698412698413\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n            <img src=\"@PROJECT_PATH@/assets/images/face-1.png\" class=\"mbr-temp\">\n        </amp-img>\n        <amp-img src=\"@PROJECT_PATH@/assets/images/face-2.png\" alt=\"image\" height=\"110\" width=\"109.78043912175649\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n            <img src=\"@PROJECT_PATH@/assets/images/face-2.png\" class=\"mbr-temp\">\n        </amp-img>\n        <amp-img src=\"@PROJECT_PATH@/assets/images/face-3.png\" alt=\"image\" height=\"110\" width=\"110\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n            <img src=\"@PROJECT_PATH@/assets/images/face-3.png\" class=\"mbr-temp\">\n        </amp-img>\n        <amp-img src=\"@PROJECT_PATH@/assets/images/face-4.png\" alt=\"image\" height=\"110\" width=\"110\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n            <img src=\"@PROJECT_PATH@/assets/images/face-4.png\" class=\"mbr-temp\">\n        </amp-img>\n        <amp-img src=\"@PROJECT_PATH@/assets/images/face-5.png\" alt=\"image\" height=\"110\" width=\"110\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n           <img src=\"@PROJECT_PATH@/assets/images/face-5.png\" class=\"mbr-temp\">\n        </amp-img>\n  